{
  "name": "@hubspot/app-functions-dev-server",
  "version": "0.7.2",
  "description": "A tool for testing HubSpot private app functions locally",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@hubspot/cli-lib": "^4.1.6",
    "body-parser": "1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/mock-fs": "^4.13.1",
    "jest": "29.5.0",
    "mock-fs": "^5.2.0",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^2.0.22",
    "ts-jest": "29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm run clean && tsc",
    "prepare": "npm run build",
    "start": "ts-node ./src/server.ts",
    "start:nodemon": "npx nodemon ./src/server.ts",
    "test": "jest",
    "watch": "tsc -- watch"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
